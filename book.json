{
  "plugins": [
    "theme-api",
    "edit-link",
    "prism",
    "-highlight",
    "github",
    "anchorjs"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/JanPeter/neo4js/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/JanPeter/neo4js/"
    },
    "theme-api": {
      "languages": [
        {
          "lang": "js",
          "name": "JavaScript",
          "default": true
        }
      ]
    }
  }
}
