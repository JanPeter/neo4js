{
  "name": "neo4-js",
  "version": "1.0.7",
  "description": "Simple neo4j-driver orm wrapper (inspired by sequelize)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JanPeter/neo4js.git"
  },
  "bugs": {
    "url": "https://github.com/JanPeter/neo4js/issues"
  },
  "keywords": [
    "neo4j",
    "orm",
    "nodejs",
    "object relational mapper"
  ],
  "author": "Jan Schlacher <j.schlacher@student.tugraz.at>",
  "license": "ISC",
  "scripts": {
    "build": "babel -d lib/ src/",
    "watch": "gulp watch",
    "nodemon": "gulp nodemon",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-register --reporter spec test/**/*.test.js test/*.test.js",
    "test:watch": "npm test -- --watch",
    "dev": "nodemon --exec \"babel-node index.js\"",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/**/*.test.js test/*.test.js",
    "doc": "gulp doc"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "immutable": "^3.8.1",
    "lodash": "^4.15.0",
    "neo4j-driver": "^1.1.0-M01",
    "node-uuid": "^1.4.7"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "cross-env": "^2.0.0",
    "docdash": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-jsdoc3": "^0.3.0",
    "gulp-nodemon": "^2.1.0",
    "istanbul": "^0.4.5",
    "jsdoc-babel": "^0.2.1",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "nyc": "^8.1.0",
    "webpack": "^1.13.2"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
