{
  "name": "neo4-js",
  "version": "2.1.3",
  "description": "Neo4j graphdb object graph mapper for javascript",
  "main": "lib/index.js",
  "author": "Jan Schlacher <j.schlacher@trisoft.at>",
  "repository": {
    "type": "git",
    "url": "https://github.com/JanPeter/neo4js"
  },
  "keywords": [
    "neo4j",
    "neo4js",
    "neo4-js",
    "orm",
    "ogm",
    "object relation mapper",
    "object graph mapper",
    "library",
    "graphdb",
    "database"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:clean && yarn build:lib && yarn build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib src --ignore '**/__tests__/**'",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "prepublish": "yarn build",
    "test": "jest --runInBand",
    "test:watch": "yarn test -- --watch",
    "start-db": "./scripts/neo4j-startup.sh",
    "prettier": "prettier --trailing-comma es5 --write 'src/**/*.js'",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "npm run docs:prepare && npm run docs:clean && gitbook build",
    "docs:publish": "npm run docs:clean && npm run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:janpeter/neo4js gh-pages --force"
  },
  "dependencies": {
    "core-decorators": "^0.19.0",
    "idx": "^1.5.0",
    "lodash": "^4.17.4",
    "neo4j-driver": "^1.4.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-idx": "^1.5.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-3": "^6.24.1",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.2.1",
    "gitbook-cli": "^2.3.2",
    "jest": "^20.0.4",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "/__tests__/.*test\\.jsx?$",
    "setupFiles": [
      "<rootDir>jest-setup.js"
    ]
  }
}
